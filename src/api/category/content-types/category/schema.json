{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "product_catalog": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product-catalog.product-catalog"
    },
    "brands": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::brand.brand",
      "inversedBy": "categories"
    },
    "description": {
      "type": "text"
    },
    "available": {
      "type": "boolean",
      "default": true,
      "required": true
    }
  }
}
